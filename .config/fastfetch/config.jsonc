{
	"$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
//	"logo": {
//		"source": "~/.config/fastfetch/logos/anime1.png",
//		"type": "kitty",
//		"height": 8,
//		"padding": {
//			"top": 0
//		}
//	},


        "logo": {
                 "source": "arch",
                 "type": "small"
        },
        
	"display": {
		"percent": { "type": 9 }, // colored percent number
		"separator": "  "
	},
	"modules": [
		{
			"type": "title",
			"color": {
				"at": "bright_black",
				"host": "bright_red",
				"user": "bright_yellow"
			}
		},
		{ "type": "separator", "string": "─" },
		{ "type": "os", "key": "󰌽" },
		{ "type": "kernel", "key": "󱗜" },
		{ "type": "packages", "key": "󰏓" },
		{ "type": "shell", "key": "" },
		{ "type": "wm", "key": "" },
		{ "type": "theme", "key": "" },
		{ "type": "terminal", "key": "" },
		{ "type": "memory", "key": "󰍛" },
		{ "type": "battery", "key": "󰂄" },
		"break",
		{ "type": "colors", "symbol": "circle" }
	]
}
