{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "segments": [
        {
          "foreground": "#0fc469",
          "style": "powerline",
          "template": "\ue235  ({{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ end }}{{ .Major }}.{{ .Minor }}{{ end }}) ",
          "type": "python"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " {{ if gt .Code 0 }}<#ff0000>\uf00d</>{{ else }}<#23d18b>\uf42e</>{{ end }} ",
          "type": "status"
        },
        {
          "foreground": "#bab02a",
          "properties": {
            "threshold": 10
          },
          "style": "plain",
          "template": " {{ if gt .Code 0 }}<#ff0000>{{ .FormattedMs }}</>{{ else }}<#23d18b>{{ .FormattedMs }}</>{{ end }} ",
          "type": "executiontime"
        },
        {
          "foreground": "#6E7FC2",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#6E7FC2",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#FFE700",
          "style": "plain",
          "template": "{{ .HEAD }} ",
          "type": "git"
        },
        {
          "foreground": "#43D426",
          "style": "plain",
          "template": "\u276f ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#5E5B73",
    "template": "\u276f ",
    "type": "text"
  },
  "version": 3
}
